{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "manifest.json#",
    "type": "object",
    "title": "manifest",
    "description": "Schema for the manifest.json file",
    "example": "resources/manifest.json",
    "required": ["url"],
    "additionalProperties": true,
    "properties": {
      "name": {
        "type": "object",
        "description": "Name of the thesaurus"
      },
      "url": {
        "type": "string",
        "description": "Url to the thesaurus configuration file"
      }
    }
  }